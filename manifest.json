{
    "name": "yt-speedup",
    "permissions": [
        "tabs",
        "storage"
    ],
    "version": "1.0",
    "manifest_version": 2,
    "content_scripts": [{
        "matches": ["*://*.youtube.com/watch*"],
        "js": ["content.js"]
    }],

    "browser_action": {
        "default_popup": "popup.html",
        "default_title": "Speed Up"
    }
}